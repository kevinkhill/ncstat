{
  "name": "ncstat",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "ncstat": "node ./cli/bin/ncstat.js",
    "build:webui:vue": "cd webui-vue && npm run build",
    "build:webui:react": "cd webui-react && npm run build",
    "clean-all": "rimraf ./node_modules && rimraf ./*/node_modules && yarn clean-e2e && yarn build-clean",
    "clean-e2e": "yarn node ./scripts/cleanE2e.js",
    "jest-coverage": "yarn jest --coverage",
    "test": "yarn lint && yarn jest",
    "watch": "yarn build && node ./scripts/watch.js",
    "watch:ts": "yarn build:ts --watch",
    "demo:simple": "node ./demo/runner.js SIMPLE.NC",
    "demo:trm": "node ./demo/runner.js TRM.NC",
    "demo:405": "node ./demo/runner.js 405.NC",
    "demo:vf10": "node ./demo/runner.js VF10.NC",
    "parser:push": "gitsbt push ncstat-parser",
    "push:parser": "git subtree pull --squash --prefix=packages/ncstat-parser/ ncstat-parser main",
    "pull:parser": "git subtree push --prefix=packages/ncstat-parser/ ncstat-parser main",
    "open:site": "npx open-cli https://ncstat.vercel.app",
    "open:github": "npx open-cli https://github.com/kevinkhill/ncstat",
    "open:netlify": "npx open-cli https://vercel.com/kevinkhill/ncstat"
  },
  "devDependencies": {
    "@types/debug": "^4.1.5",
    "@types/jest": "^26.0.15",
    "@types/lodash": "^4.14.149",
    "@typescript-eslint/eslint-plugin": "^4.8.0",
    "@typescript-eslint/parser": "^4.8.0",
    "@vercel/node": "^1.8.5",
    "eslint": "^7.13.0",
    "eslint-config-node": "^4.1.0",
    "eslint-config-prettier": "^6.15.0",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-import-resolver-typescript": "^2.3.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jest": "^24.1.3",
    "eslint-plugin-jest-formatting": "^2.0.0",
    "eslint-plugin-lodash-fp": "^2.2.0-a1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-simple-import-sort": "^6.0.1",
    "eslint-plugin-vue": "^7.1.0",
    "git-subtree": "^0.2.1",
    "husky": "^4.3.0",
    "jest": "^26.6.3",
    "lerna": "^3.22.1",
    "lint-staged": "^10.5.1",
    "prettier": "^2.1.2",
    "ts-jest": "^26.4.4",
    "typescript": "^4.1.2",
    "ultra-runner": "^3.5.0",
    "vercel": "^21.0.1"
  }
}

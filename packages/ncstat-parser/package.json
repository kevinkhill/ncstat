{
  "name": "@ncstat/parser",
  "version": "1.0.3",
  "description": "Static NC file parser",
  "author": "Kevin Hill <kevinkhill@gmail.com> (https://github.com/kevinkhill)",
  "license": "MIT",
  "private": false,
  "source": "src/index.ts",
  "main": "build/index.js",
  "unpkg": "build/index.js",
  "cdnjs": "build/index.js",
  "browser": "build/index.js",
  "types": "build/index.d.ts",
  "repository": {
    "type": "git",
    "url": "https://github.com/kevinkhill/ncstat.git",
    "directory": "packages/ncstat-parser"
  },
  "scripts": {
    "build": "tsc --project tsconfig.build.json",
    "clean": "rimraf ./build/*",
    "demo": "node ./demo.js",
    "dev": "tsc-watch --project tsconfig.build.json --onSuccess \"node ./demo.js\"",
    "start": "yarn build && yarn demo",
    "test": "jest",
    "lint": "eslint 'src/**/*.ts'",
    "prebuild": "yarn clean",
    "fix": "yarn lint --fix"
  },
  "lint-staged": {
    "*.ts": "eslint --cache --fix './src/**/*.ts'"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "dependencies": {
    "@xstate/fsm": "^1.5.1",
    "debug": "^4.1.1",
    "emittery": "^0.7.2",
    "lodash": "^4.17.20",
    "ts-tokenizr": "^1.1.1"
  },
  "devDependencies": {
    "@types/jest": "^26.0.23",
    "@typescript-eslint/eslint-plugin": "^4.8.1",
    "@typescript-eslint/parser": "^4.8.1",
    "eslint": "^7.13.0",
    "eslint-config-prettier": "^6.15.0",
    "eslint-import-resolver-typescript": "^2.3.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jest-formatting": "^3.0.0",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-simple-import-sort": "^6.0.0",
    "jest": "^26.6.3",
    "nodemon": "^2.0.6",
    "np": "^7.0.0",
    "prettier": "^2.1.2",
    "rollup": "^2.33.3",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-json": "^4.0.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-sourcemaps": "^0.6.3",
    "rollup-plugin-typescript2": "^0.29.0",
    "ts-jest": "^26.5.6",
    "ts-node": "^9.0.0",
    "ts-node-dev": "^1.0.0",
    "tsc-watch": "^4.2.9",
    "typescript": "^4.1.2",
    "vercel": "^21.0.1"
  }
}
